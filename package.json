    "leaflet": "1.6.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "enzyme": "3.11.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsdoc": "20.0.0",
    "eslint-plugin-mocha": "6.2.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "jest": "27.0.6",
    "parcel": "2.0.0-rc.0",
    "postcss": "8.2.15",
